{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "patch": "npm version patch -git-tag-version false",
    "start": "expo start",
    "eject": "expo eject",
    "lint": "eslint *.js **/*.js App.js",
    "fix": "eslint --fix --ext .js,.jsx,.ts,.tsx ./",
    "minor_update": "npm version minor -git-tag-version false && git add . && git commit -m \"Update\" && git push",
    "build and": "eas build --platform android",
    "build ios": "eas build --platform ios",
    "build all": "eas build --platform all"
  },
  "dependencies": {
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.4",
    "expo": "^45.0.0",
    "expo-mail-composer": "~11.2.0",
    "expo-sqlite": "~10.2.0",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.1",
    "expo-web-browser": "~10.2.0",
    "fuse.js": "^6.5.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.1",
    "react-native-animatable": "^1.3.3",
    "react-native-collapsible": "^1.6.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-searchable-dropdown": "^1.1.3",
    "react-native-svg": "12.3.0",
    "react-native-svg-transformer": "^0.14.3",
    "react-native-vector-icons": "^9.1.0",
    "react-native-web": "0.17.7"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/eslint-parser": "^7.17.0",
    "eslint": "8.15.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-native": "^4.0.0"
  },
  "private": true,
  "version": "0.2.0"
}
